{% extends '::base.html.twig' %}

{% block body -%}
    <h1>Telefono</h1>

    <table class="record_properties">
        <tbody>
            <tr>
                <th>Id</th>
                <td>{{ entity.id }}</td>
            </tr>
            <tr>
                <th>Numero</th>
                <td>{{ entity.numero }}</td>
            </tr>
            <tr>
                <th>Anexo</th>
                <td>{{ entity.anexo }}</td>
            </tr>
            <tr>
                <th>Fechaalta</th>
                <td>{{ entity.fechaAlta|date('Y-m-d H:i:s') }}</td>
            </tr>
            <tr>
                <th>Tipotelefono</th>
                <td>{{ entity.tipoTelefono }}</td>
            </tr>
            <tr>
                <th>Cuspp</th>
                <td>{{ entity.cuspp }}</td>
            </tr>
            <tr>
                <th>Estado</th>
                <td>{{ entity.estado }}</td>
            </tr>
        </tbody>
    </table>

        <ul class="record_actions">
    <li>
        <a href="{{ path('telefono') }}">
            Back to the list
        </a>
    </li>
    <li>
        <a href="{{ path('telefono_edit', { 'id': entity.id }) }}">
            Edit
        </a>
    </li>
    <li>{{ form(delete_form) }}</li>
</ul>
{% endblock %}
